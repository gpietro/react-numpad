{
  "name": "react-numpad",
  "version": "5.0.3",
  "description": "Numpad react component, as number input or time input",
  "homepage": "https://gpietro.github.io/react-numpad-demo/",
  "main": "build/index.js",
  "types": "./index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/gpietro/react-numpad"
  },
  "scripts": {
    "start": "webpack --watch",
    "build": "babel lib --ignore **/__tests__/*.js -d build",
    "build:watch": "babel lib -w --ignore **/__tests__/*.js -d build",
    "lint": "eslint lib/** --fix; exit 0",
    "lint:watch": "esw -w lib/**",
    "prepublish": "npm run build",
    "storybook": "start-storybook -p 6006 -c .storybook",
    "test": "jest --verbose false",
    "test:watch": "jest --watch"
  },
  "lint-staged": {
    "*.{js,json,css,md}": [
      "prettier",
      "git add"
    ]
  },
  "author": "Pietro Ghezzi",
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "^7.7.0",
    "@babel/core": "^7.7.2",
    "@babel/preset-env": "^7.7.1",
    "@babel/preset-react": "^7.7.0",
    "@storybook/addon-actions": "^5.2.5",
    "@storybook/addon-links": "^5.2.5",
    "@storybook/react": "^5.2.5",
    "@testing-library/jest-dom": "^5.0.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.3",
    "babel-jest": "^24.9.0",
    "babel-loader": "^8.0.6",
    "chai": "^4.2.0",
    "css-loader": "^3.2.0",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.15.1",
    "eslint": "^6.6.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-prettier": "^6.5.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-react": "^7.16.0",
    "eslint-plugin-react-hooks": "^2.2.0",
    "eslint-watch": "^6.0.1",
    "file-loader": "^4.2.0",
    "husky": "^3.0.9",
    "jest": "^24.9.0",
    "jest-dom": "^4.0.0",
    "jest-styled-components": "^6.3.3",
    "lint-staged": "^9.4.2",
    "node-sass": "^4.13.0",
    "polished": "^3.4.2",
    "prettier": "^1.18.2",
    "react": "^16.11.0",
    "react-dom": "^16.11.0",
    "react-test-renderer": "^16.11.0",
    "sass-loader": "^8.0.0",
    "sinon": "^7.5.0",
    "sinon-sandbox": "^2.0.6",
    "storybook-addon-specifications": "^2.1.5",
    "style-loader": "^1.0.0",
    "url-loader": "^2.2.0",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.10"
  },
  "peerDependencies": {
    "react": "^16.8.6",
    "react-dom": "^16.8.6"
  },
  "dependencies": {
    "@material-ui/core": "^4.6.0",
    "@material-ui/icons": "^4.5.1",
    "@storybook/addon-knobs": "^5.2.5",
    "@testing-library/dom": "^6.10.1",
    "@testing-library/react": "^9.3.2",
    "color": "^3.1.2",
    "lodash.chunk": "^4.2.0",
    "lodash.isequal": "^4.5.0",
    "moment": "^2.24.0",
    "prop-types": "^15.7.2",
    "react-portal": "^4.2.0",
    "react-swipeable": "^5.4.0",
    "styled-components": "^4.4.1",
    "use-onclickoutside": "^0.3.1"
  },
  "prettier": {
    "printWidth": 100,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}