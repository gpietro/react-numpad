{
  "name": "react-numpad",
  "version": "4.1.2",
  "description": "Numpad react component, as number input or time input",
  "homepage": "https://gpietro.github.io/docs/",
  "main": "build/index.js",
  "types": "./index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/gpietro/react-numpad"
  },
  "scripts": {
    "start": "webpack --watch",
    "build": "babel lib --ignore **/__tests__/*.js -d build",
    "build:watch": "babel lib -w --ignore **/__tests__/*.js -d build",
    "lint": "eslint lib/** --fix; exit 0",
    "lint:watch": "esw -w lib/**",
    "prepublish": "npm run build",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "lint-staged": {
    "*.{js,json,css,md}": [
      "prettier",
      "git add"
    ]
  },
  "author": "Pietro Ghezzi",
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.2.2",
    "@babel/preset-env": "^7.3.1",
    "@babel/preset-react": "^7.0.0",
    "@storybook/addon-actions": "^4.1.11",
    "@storybook/addon-knobs": "^4.1.11",
    "@storybook/addon-links": "^4.1.11",
    "@storybook/react": "^4.1.11",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.1.0",
    "babel-loader": "^8.0.5",
    "chai": "^4.2.0",
    "css-loader": "^2.1.0",
    "enzyme": "^3.8.0",
    "enzyme-adapter-react-16": "^1.9.1",
    "eslint": "^5.14.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^4.0.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "^7.12.4",
    "eslint-plugin-react-hooks": "^1.0.1",
    "eslint-watch": "^4.0.2",
    "file-loader": "^3.0.1",
    "husky": "^1.3.1",
    "jest": "^24.1.0",
    "jest-styled-components": "^6.3.1",
    "lint-staged": "^8.1.3",
    "node-sass": "^4.11.0",
    "polished": "^3.0.1",
    "prettier": "^1.16.4",
    "react": "^16.8.3",
    "react-dom": "^16.8.3",
    "react-test-renderer": "^16.8.0",
    "sass-loader": "^7.1.0",
    "sinon": "^7.2.3",
    "sinon-sandbox": "^2.0.0",
    "storybook-addon-specifications": "^2.1.2",
    "style-loader": "^0.23.1",
    "url-loader": "^1.1.2",
    "webpack": "^4.29.3",
    "webpack-cli": "^3.2.3"
  },
  "peerDependencies": {
    "react": "^16.8.0",
    "react-dom": "^16.8.0"
  },
  "dependencies": {
    "@material-ui/core": "^3.9.2",
    "color": "^3.1.0",
    "lodash": "^4.17.11",
    "moment": "^2.24.0",
    "prop-types": "^15.6.2",
    "react-icons": "2.2.7",
    "react-onclickoutside": "^6.7.1",
    "react-portal": "^4.2.0",
    "react-swipeable": "^4.3.2",
    "styled-components": "^4.1.3"
  },
  "prettier": {
    "printWidth": 100,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
