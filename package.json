    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "moment": "2.19.1"
  },
  "dependencies": {
    "@storybook/react": "^3.3.10",
    "bootstrap": "^4.0.0-beta.2",
    "color": "^2.0.1",
    "prop-types": "^15.6.0",
    "react-icons": "2.2.7",
    "react-onclickoutside": "^6.7.1",
    "react-portal": "^4.1.2",
    "react-swipeable": "^4.2.0",
    "react-transition-group": "^2.2.1",
    "reactstrap": "^5.0.0-alpha.4",
    "styled-components": "^3.0.2"
